{
    "id": "car",
    "name": {
        "en": "Car",
        "de": "Fahrzeug",
        "nl": "Voertuig"
    },
    "images": {
        "large": "/drivers/car/assets/images/large.png",
        "small": "/drivers/car/assets/images/small.png"
    },
    "class": "sensor",
    "connectivity": [ "cloud" ],
    "capabilities": [
        "measure_battery",

        "last_update",
        "state",

        "meter_odo",
        "measure_car_software_version",

        "measure_car_software_update_version",
        "software_update_state",

        "measure_drive_shift_state",
        "measure_drive_speed",

        "measure_car_tpms_pressure_fl",
        "measure_car_tpms_pressure_fr",
        "measure_car_tpms_pressure_rl",
        "measure_car_tpms_pressure_rr",


        "alarm_api_error",
        "measure_api_error",

        "refresh",
        "wake_up",

        "car_doors_locked",
        "car_sentry_mode"
        
    ],
    "energy": {
        "batteries": [
            "OTHER"
        ]
    },
    "pair": [
        {
            "id": "login_oauth2",
            "template": "login_oauth2"
        },
        {
            "id": "list_devices",
            "template": "list_devices",
            "navigation": {
                "next": "add_devices"
            }
        },
        {
            "id": "add_devices",
            "template": "add_devices"
        },
        {
            "id": "register",
            "navigation": { 
                "next": "done" 
            }
        },
        {
            "id": "done",
            "template": "done"
        }
    ],
    "repair": [
        {
            "id": "select"
        },
        {
            "id": "login_oauth2",
            "template": "login_oauth2"
        },
        {
            "id": "register",
            "navigation": { 
                "next": "done"
            }
        },
        {
            "id": "done",
            "template": "done"
        }
    ]
}